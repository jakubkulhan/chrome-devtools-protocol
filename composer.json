{
  "name": "jakubkulhan/chrome-devtools-protocol",
  "type": "library",
  "description": "Chrome Devtools Protocol client for PHP",
  "keywords": [
    "chrome",
    "chrome-devtools",
    "chrome-devtools-protocol",
    "headless-chrome",
    "web",
    "automation"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Jakub Kulhan",
      "email": "jakub.kulhan@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.0",
    "php": "~7.1",
    "symfony/filesystem": ">=2.3",
    "symfony/process": ">=3.3",
    "wrench/wrench": "~2.0.10"
  },
  "require-dev": {
    "jakub-onderka/php-parallel-lint": "~1.0.0",
    "nette/php-generator": "~2.1|~3.0",
    "phpstan/phpstan": "^0.11.4",
    "phpunit/phpunit": "~6.4"
  },
  "autoload": {
    "psr-4": {
      "ChromeDevtoolsProtocol\\": [
        "src/ChromeDevtoolsProtocol/",
        "gen-src/ChromeDevtoolsProtocol/"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ChromeDevtoolsProtocol\\": [
        "test/ChromeDevtoolsProtocol/"
      ]
    }
  }
}
